{
  "name": "github-insights",
  "description": "Insights into a GitHub profile",
  "repository": "github:jsulpis/github-insights",
  "license": "MIT",
  "author": "Julien Sulpis",
  "scripts": {
    "dev": "next src",
    "build": "npm run clean && next build src",
    "build:now": "npm run build && cp -r src/.next .next && cp -r src/public public",
    "export": "next export src",
    "start": "next start src",
    "clean": "rm -rf .next src/.next public src/out",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "BUNDLE_ANALYZE=both npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "dotenv": "^8.2.0",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.3.2",
    "next-page-transitions": "^1.0.0-beta.2",
    "paper-dashboard-react": "^1.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.9.2",
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "bootstrap": "^4.4.1",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "next-fonts": "^0.19.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "reactstrap": "^8.1.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "typescript": "^3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
